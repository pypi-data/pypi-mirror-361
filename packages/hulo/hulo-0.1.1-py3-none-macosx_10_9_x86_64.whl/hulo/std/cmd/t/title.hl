// Creates a title for the Command Prompt window.
@command("title [string]")
pub class title {
    // Specifies the text to appear as the title of the Command Prompt window.
    @option(bind: "string")
    string: str?

    // Displays help at the command prompt.
    @flag(short: "h", long: "help", format: "/?")
    help: bool?
}