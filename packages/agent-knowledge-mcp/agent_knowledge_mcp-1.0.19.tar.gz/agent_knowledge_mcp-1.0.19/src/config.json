{
  "elasticsearch": {
    "host": "localhost",
    "port": 9200,
    "auto_setup": true
  },
  "security": {
    "allowed_base_directory": "./.knowledges/docs"
  },
  "document_validation": {
    "strict_schema_validation": "true",
    "allow_extra_fields": false,
    "required_fields_only": false,
    "auto_correct_paths": true
  },
  "version_control": {
    "enabled": true,
    "type": "git",
    "auto_commit": false
  },
  "server": {
    "name": "AgentKnowledgeMCP",
    "version": "1.0.19"
  }
}