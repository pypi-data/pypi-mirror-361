<data>
<record id="liquidity_transfer_open_2_done" model="base.automation">
    <field name="action_server_id" ref="liquidity_transfer_action_done" />
    <field name="trigger">on_write</field>
    <field name="filter_pre_domain">[('debit_credit_realized','=',False)]</field>
    <field name="filter_domain">[('debit_credit_realized','=',True)]</field>
</record>

<record id="liquidity_transfer_done_2_open" model="base.automation">
    <field name="action_server_id" ref="liquidity_transfer_action_open" />
    <field name="trigger">on_write</field>
    <field name="filter_pre_domain">[('debit_credit_realized','=',True)]</field>
    <field name="filter_domain">[('debit_credit_realized','=',False)]</field>
</record>
</data>
</odoo>
