        "annotation_types":{
            "box" :{
                "description" : "rectangular bounding box, [x1,y1,x2,y2] for the coordinates"
            },
            "circle" : {
                "description" : "TBI : circle [x,y,r] for the coordinates"
            },
            "poligon" : {
                "description" :  "TBI : polygon [[x1,y1],[x2,y2],[x3,y3],...]  for the coordinates"
            },
            "points" : {
                "description" :  "TBI : set of points [[x1,y1],[x2,y2],[x3,y3],...]  for the coordinates"
            },
            "directed-line" :{
                "description" : "TBI : directed line from one point to the other. [[x0,y0],[x1,y1]] for vector coordinate stemming from [x0,y0]"
            },
            "lines":{
                "description" : "TBI : set of lines. [[x1,y1,x2,y2], [x3,y3,x4,y4] , ... ] for the coordinates"
            },
            "skeleton" : {
                "description" : "TBI : skeleton points [[x1,y1],[x2,y2],[x3,y4], ... ] for the points of the skeleton"
            }
        },
        "score_types":{
            "integer-set" :{
                "description" : "Set of integers for labels such as [1,2,3]"
            },
            "integer-ranges" :{
                "description" : "Set of ranges for values such as [ [1,3] , [9,12] ] meaning the union of two integer ranges equivalent to [1,2,3,9,10,11,12]"
            },
            "float-set" :{
                "description" : "Set of floating numbers for classes such as [0.4,1.5,3.4]"
            },
            "float-ranges":{
                "description" : "Set of ranges of the floating numbers such as [ [1.0,2.5], [4.2,5.8] ] implying a number in the union of those two intervals"
            }
        }
    },

