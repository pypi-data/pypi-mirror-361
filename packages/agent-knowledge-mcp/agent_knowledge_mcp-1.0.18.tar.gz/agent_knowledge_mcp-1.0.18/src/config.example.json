{
  "elasticsearch": {
    "host": "localhost",
    "port": 9200,
    "timeout": 30,
    "auto_setup": true,
    "docker_image": "docker.elastic.co/elasticsearch/elasticsearch:8.14.1"
  },
  "kibana": {
    "enabled": true,
    "host": "localhost", 
    "port": 5601,
    "docker_image": "docker.elastic.co/kibana/kibana:8.14.1"
  },
  "security": {
    "allowed_base_directory": "/Your-Documents-Path",
    "allowed_subdirectories": [
      "documents",
      "uploads",
      "exports"
    ]
  },
  "server": {
    "name": "AgentKnowledgeMCP",
    "version": "0.1.0",
    "max_file_size_mb": 100,
    "enable_file_operations": true,
    "auto_setup_elasticsearch": true
  },
  "logging": {
    "level": "INFO",
    "enable_debug": false
  }
}
