<?xml version="1.0" encoding="UTF-8"?>
<object xmlns="http://textgrid.info/namespaces/metadata/core/2010" xmlns:tg="http://textgrid.info/relation-ns#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:tei="http://www.tei-c.org/ns/1.0">
    <generic>
        <provided>
{% if work.title %}
            <title>{{ work.title }}</title>
{% endif %}
            <format>text/xml</format>
        </provided>
    </generic>
{% if rights_holder and rights_holder[0].fullname %}
    <item>
    {% for item in rights_holder %}
        {% if item.fullname %}
            {% if item.url %}
        <rightsHolder id="{{ item.url }}">{{ item.fullname }}</rightsHolder>
            {% else %}
        <rightsHolder>{{ item.fullname }}</rightsHolder>
            {% endif %}
        {% endif %}
    {% endfor %}
    </item>
{% endif %}
</object>