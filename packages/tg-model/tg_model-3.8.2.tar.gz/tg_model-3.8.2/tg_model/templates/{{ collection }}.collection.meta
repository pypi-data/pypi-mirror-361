<?xml version="1.0" encoding="UTF-8"?>
<object xmlns="http://textgrid.info/namespaces/metadata/core/2010" xmlns:tg="http://textgrid.info/relation-ns#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <generic>
        <provided>
{% if title %}
            <title>{{ title }}</title>
{% endif %}
            <format>text/tg.collection+tg.aggregation+xml</format>
        </provided>
    </generic>
    <collection>
    {% for item in collectors %}
        {% if item.fullname %}
            {% if item.url %}
        <collector id="{{ item.url }}">{{ item.fullname }}</collector>
            {% else %}
        <collector>{{ item.fullname }}</collector>
            {% endif %}
        {% endif %}
    {% endfor %}
    </collection>
</object>