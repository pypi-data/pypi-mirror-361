// @hulo-build bash
pub class DateTime {
    DateTime(
        year: num, month: num, day: num,
        hour?: num = 0, minute?: num = 0, 
        second?: num = 0, millisec?: num = 0, 
        nanosec?: num = 0
    ) {
        return unsafe { $(date -d "{{$year}}-{{$month}}-{{$day}}" +%s) }
    }

    DateTime.now() {
        return unsafe { $(date +%s) };
    }

    operator -(other: DateTime) {
        return unsafe { $((($this - $other) / 86400)) };
    }
}