[Unit]
Description="{{ pine_name }}-redis-queue"
PartOf={{ pine_name }}-redis.target

[Service]
User={{ user }}
Group={{ user }}
Restart=always
ExecStart={{ redis_server }} {{ redis_queue_config }}
StandardOutput=file:{{ pine_dir }}/logs/redis-queue.log
StandardError=file:{{ pine_dir }}/logs/redis-queue.error.log
WorkingDirectory={{ sites_dir }}
