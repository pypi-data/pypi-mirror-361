/// Sums an array's contents
pub fn math_sum(list: num[]) -> num

/// Returns a number, rounded to the nearest integer
pub fn math_round(number: num) -> num

/// Returns the largest integer less than or equal to a number
pub fn math_floor(number: num) -> num

/// Returns the smallest integer greater than or equal to a number
pub fn math_ceil(number: num) -> num

/// Returns the absolute value of a number
pub fn math_abs(number: num) -> num